{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "DocumentTouch": "readonly",
        "$": "readonly",
        "_": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "space-before-function-paren": ["error", "never"],
        "camelcase": "off",
        "space-in-parens": "off"
    }
}